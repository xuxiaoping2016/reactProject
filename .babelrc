{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
              "legacy": true
            }
        ],
        [
            "babel-plugin-import",
            {
              "libraryName": "antd",
              "libraryDirectory": "es",
              "style": true
            },
            "ant"
        ]
    ]
}